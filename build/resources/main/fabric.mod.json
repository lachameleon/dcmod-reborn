{
	"schemaVersion": 1,
	"id": "discord-chat-integration",
	"version": "1.0.2",
	"name": "Discord Chat Integration",
	"description": "Send Minecraft commands/messages from Discord",
	"authors": [
		"Aurick"
	],
	"contact": {
		"homepage": "https://github.com/aurickk/Discord-Chat-Integration/",
		"sources": "https://github.com/aurickk/Discord-Chat-Integration/",
		"issues": "https://github.com/aurickk/Discord-Chat-Integration/issues"
	},
	"icon": "assets/discord-chat-integration/icon.png",
